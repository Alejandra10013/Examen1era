@model Tienda.Models.Clientes
<h2>Ficha Clientes</h2>
<fieldset class="form-horizontal">
    <legend class="label">Informacion de Clientes</legend>
    <label class="control-label">Id</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.Id)</div>
    </div>
    <label class="control-label">Rut</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.Rut)</div>
    </div>
    <label class="control-label">Nombre del Clientes</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.NombreCli)</div>
    </div>
    <label class="control-label">Apellido</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.Apellido)</div>
    </div>
    <label class="control-label">Direccion</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.Direccion)</div>
    </div>
    <label class="control-label">Tipo</label>
    <div class="form-group">
        <div class="text-muted">@Html.DisplayFor(m => m.TipoCliente)</div>
    </div>
</fieldset>